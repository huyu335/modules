nextflow_process {

    name "Test Process PHYLOWGSPARSECNVS"
    script "../main.nf"
    process "PHYLOWGSPARSECNVS"

    tag "modules"
    tag "modules_nfcore"
    tag "modules_msk"
    tag "phylowgsparsecnvs"

    test("phylowgsparsecnvs - gene_level") {

        when {
            process {
                """                
                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file(params.test_data_mskcc['neoantigen']['facets_gene_level_txt'], checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }

            )
        }

    }

    test("phylowgsparsecnvs - gene_level") {

        options "-stub"

        when {
            process {
                """
                // TODO nf-core: define inputs of the process here. Example:
                
                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file(params.test_data_mskcc['neoantigen']['facets_gene_level.txt'], checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
